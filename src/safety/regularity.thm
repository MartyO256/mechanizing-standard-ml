schema tm-of-reg-ctx =
  % conbind
  some [K : kind]
  block (a : con, da : cn-of a K) +
  % conbind-reg
  some [K : kind, wf : kd-wf K]
  block (a : con, da : cn-of a K) +
  % termbind-reg
  some [T : con, Dwf : cn-of T t]
