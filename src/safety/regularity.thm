% Constructor Regularity

schema conbind-reg =
  % conbind
  % FIXME: Redundant if schemas are treated as lattices
  some [K : kind]
  block (a : con, da : cn-of a K) +
  % conbind-reg
  some [K : kind, wf : kd-wf K]
  block (a : con, da : cn-of a K);

LF kd-equiv-reg/e : kind -> kind -> type =
| kd-equiv-reg/i : kd-wf K1 -> kd-wf K2 -> kd-equiv-reg/e K1 K2
;

LF kd-sub-reg/e : kind -> kind -> type =
| kd-sub-reg/i : kd-wf K1 -> kd-wf K2 -> kd-sub-reg/e K1 K2
;

LF cn-equiv-reg/e :  con -> con -> kind -> type =
| cn-equiv-reg/i : cn-of C1 K -> cn-of C2 K -> kd-wf K -> cn-equiv-reg/e C1 C2 K
;

proof kd-equiv-reg :
  (g : conbind-reg)
  [g |- kd-equiv K1 K2] ->
    [g |- kd-equiv-reg/e K1 K2] =
/ total 1 /
intros
{ g : conbind-reg, K1 : (g |- kind), K2 : (g |- kind)
| x : [g |- kd-equiv K1 K2]
; split x as
  case kd-equiv/sigma:
  { g : conbind-reg,
    K3 : (g |- kind),
    K5 : (g, a7 : con |- kind),
    K4 : (g |- kind),
    K6 : (g, a8 : con |- kind),
    Dequiv : (g |- kd-equiv K3 K4),
    Dequiv1 :
      (g, a : con, z586 : cn-of a (K3[..]) |-
         kd-equiv (K5[.., a]) (K6[.., a]))
  | x : [g |- kd-equiv (sigma K3 (\z522. K5)) (sigma K4 (\z522. K6))]
  ; split kd-equiv-reg [_ |- Dequiv] as
    case kd-equiv-reg/i:
    { g : conbind-reg,
      K3 : (g |- kind),
      K5 : (g, a7 : con |- kind),
      K4 : (g |- kind),
      K6 : (g, a8 : con |- kind),
      Dequiv : (g |- kd-equiv K3 K4),
      Dequiv1 :
        (g, a : con, z586 : cn-of a (K3[..]) |-
           kd-equiv (K5[.., a]) (K6[.., a])),
      Dwf : (g |- kd-wf K3),
      Dwf1 : (g |- kd-wf K4)
    | x : [g |- kd-equiv (sigma K3 (\z522. K5)) (sigma K4 (\z522. K6))]
    ; split
      kd-equiv-reg
        [_, b : block (a : con, da : cn-of a (K3[..])) |-
           Dequiv1[.., b.1, b.2]] as
      case kd-equiv-reg/i:
      { g : conbind-reg,
        K3 : (g |- kind),
        K7 : (g, a : con |- kind),
        K4 : (g |- kind),
        K8 : (g, a : con |- kind),
        Dequiv : (g |- kd-equiv K3 K4),
        Dequiv1 :
          (g, a : con, z586 : cn-of a (K3[..]) |-
             kd-equiv (K7[.., a]) (K8[.., a])),
        Dwf : (g |- kd-wf K3),
        Dwf1 : (g |- kd-wf K4),
        Dwf2 : (g, a : con, z5 : cn-of a (K3[..]) |- kd-wf (K7[.., a])),
        Dwf3 : (g, a : con, y6 : cn-of a (K3[..]) |- kd-wf (K8[.., a]))
      | x : [g |- kd-equiv (sigma K3 (\z522. K7)) (sigma K4 (\z522. K8))]
      ; solve
          [_ |-
             kd-equiv-reg/i (kd-wf/sigma Dwf (\a. \da. Dwf2))
             (kd-wf/sigma Dwf1
             (\a.
                \da.
                  Dwf3[.., a, cn-of/subsume da
                       (kd-sub/refl (kd-equiv/symm (Dequiv[..])))]))]
      }
    }
  }
  case kd-equiv/pi:
  { g : conbind-reg,
    K3 : (g |- kind),
    K5 : (g, a5 : con |- kind),
    K4 : (g |- kind),
    K6 : (g, a6 : con |- kind),
    Dequiv : (g |- kd-equiv K3 K4),
    Dequiv1 :
      (g, a : con, z585 : cn-of a (K3[..]) |-
         kd-equiv (K5[.., a]) (K6[.., a]))
  | x : [g |- kd-equiv (pi K3 (\z521. K5)) (pi K4 (\z521. K6))]
  ; split kd-equiv-reg [g |- Dequiv] as
    case kd-equiv-reg/i:
    { g : conbind-reg,
      K3 : (g |- kind),
      K5 : (g, a5 : con |- kind),
      K4 : (g |- kind),
      K6 : (g, a6 : con |- kind),
      Dequiv : (g |- kd-equiv K3 K4),
      Dequiv1 :
        (g, a : con, z585 : cn-of a (K3[..]) |-
           kd-equiv (K5[.., a]) (K6[.., a])),
      Dwf : (g |- kd-wf K3),
      Dwf1 : (g |- kd-wf K4)
    | x : [g |- kd-equiv (pi K3 (\z521. K5)) (pi K4 (\z521. K6))]
    ; split
      kd-equiv-reg
        [g, b : block (a : con, da : cn-of a (K3[..])) |-
           Dequiv1[.., b.1, b.2]] as
      case kd-equiv-reg/i:
      { g : conbind-reg,
        K3 : (g |- kind),
        K7 : (g, a : con |- kind),
        K4 : (g |- kind),
        K8 : (g, a : con |- kind),
        Dequiv : (g |- kd-equiv K3 K4),
        Dequiv1 :
          (g, a : con, z585 : cn-of a (K3[..]) |-
             kd-equiv (K7[.., a]) (K8[.., a])),
        Dwf : (g |- kd-wf K3),
        Dwf1 : (g |- kd-wf K4),
        Dwf2 : (g, a : con, x991 : cn-of a (K3[..]) |- kd-wf (K7[.., a])),
        Dwf3 : (g, a : con, z991 : cn-of a (K3[..]) |- kd-wf (K8[.., a]))
      | x : [g |- kd-equiv (pi K3 (\z521. K7)) (pi K4 (\z521. K8))]
      ; solve
          [g |-
             kd-equiv-reg/i (kd-wf/pi Dwf (\a. \da. Dwf2))
             (kd-wf/pi Dwf1
             (\a.
                \da.
                  Dwf3[.., a, cn-of/subsume da
                       (kd-sub/refl (kd-equiv/symm (Dequiv[..])))]))]
      }
    }
  }
  case kd-equiv/sing:
  { g : conbind-reg,
    C : (g |- con),
    C1 : (g |- con),
    Dequiv : (g |- cn-equiv C C1 t)
  | x : [g |- kd-equiv (sing C) (sing C1)]
  ; split cn-equiv-reg [g |- Dequiv] as
    case cn-equiv-reg/i:
    { g : conbind-reg,
      C : (g |- con),
      C1 : (g |- con),
      Dequiv : (g |- cn-equiv C C1 t),
      Dof : (g |- cn-of C t),
      Dof1 : (g |- cn-of C1 t),
      Dwf : (g |- kd-wf t)
    | x : [g |- kd-equiv (sing C) (sing C1)]
    ; solve [g |- kd-equiv-reg/i (kd-wf/sing Dof) (kd-wf/sing Dof1)]
    }
  }
  case kd-equiv/trans:
  { g : conbind-reg,
    K1 : (g |- kind),
    K2 : (g |- kind),
    K4 : (g |- kind),
    Dequiv : (g |- kd-equiv K1 K4),
    Dequiv1 : (g |- kd-equiv K4 K2)
  | x : [g |- kd-equiv K1 K2]
  ; split kd-equiv-reg [g |- Dequiv] as
    case kd-equiv-reg/i:
    { g : conbind-reg,
      K1 : (g |- kind),
      K2 : (g |- kind),
      K4 : (g |- kind),
      Dequiv : (g |- kd-equiv K1 K4),
      Dequiv1 : (g |- kd-equiv K4 K2),
      Dwf : (g |- kd-wf K1),
      Dwf1 : (g |- kd-wf K4)
    | x : [g |- kd-equiv K1 K2]
    ; split kd-equiv-reg [g |- Dequiv1] as
      case kd-equiv-reg/i:
      { g : conbind-reg,
        K1 : (g |- kind),
        K2 : (g |- kind),
        K4 : (g |- kind),
        Dequiv : (g |- kd-equiv K1 K4),
        Dequiv1 : (g |- kd-equiv K4 K2),
        Dwf : (g |- kd-wf K1),
        Dwf1 : (g |- kd-wf K4),
        Dwf2 : (g |- kd-wf K4),
        Dwf3 : (g |- kd-wf K2)
      | x : [g |- kd-equiv K1 K2]
      ; solve [g |- kd-equiv-reg/i Dwf Dwf3]
      }
    }
  }
  case kd-equiv/symm:
  { g : conbind-reg,
    K1 : (g |- kind),
    K2 : (g |- kind),
    Dequiv : (g |- kd-equiv K2 K1)
  | x : [g |- kd-equiv K1 K2]
  ; split kd-equiv-reg [g |- Dequiv] as
    case kd-equiv-reg/i:
    { g : conbind-reg,
      K1 : (g |- kind),
      K2 : (g |- kind),
      Dequiv : (g |- kd-equiv K2 K1),
      Dwf : (g |- kd-wf K2),
      Dwf1 : (g |- kd-wf K1)
    | x : [g |- kd-equiv K1 K2]
    ; solve [g |- kd-equiv-reg/i Dwf1 Dwf]
    }
  }
  case kd-equiv/refl:
  { g : conbind-reg, K2 : (g |- kind), Dwf : (g |- kd-wf K2)
  | x : [g |- kd-equiv K2 K2]
  ; solve [g |- kd-equiv-reg/i Dwf Dwf]
  }
}

and proof kd-sub-reg :
  (g : conbind-reg)
  [g |- kd-sub K1 K2] ->
    [g |- kd-sub-reg/e K1 K2] =
/ total 1 /
intros
{ g : conbind-reg, K1 : (g |- kind), K2 : (g |- kind)
| x : [g |- kd-sub K1 K2]
; split x as
  case kd-sub/sigma:
  { g : conbind-reg,
    K3 : (g |- kind),
    K5 : (g, a11 : con |- kind),
    K4 : (g |- kind),
    K6 : (g, a12 : con |- kind),
    Dsub : (g |- kd-sub K3 K4),
    Dsub1 :
      (g, a : con, x591 : cn-of a (K3[..]) |- kd-sub (K5[.., a]) (K6[.., a])),
    Dwf : (g, a : con, y591 : cn-of a (K4[..]) |- kd-wf (K6[.., a]))
  | x : [g |- kd-sub (sigma K3 (\z522. K5)) (sigma K4 (\z522. K6))]
  ; split kd-sub-reg [_ |- Dsub] as
    case kd-sub-reg/i:
    { g : conbind-reg,
      K3 : (g |- kind),
      K5 : (g, a11 : con |- kind),
      K4 : (g |- kind),
      K6 : (g, a12 : con |- kind),
      Dsub : (g |- kd-sub K3 K4),
      Dsub1 :
        (g, a : con, x591 : cn-of a (K3[..]) |-
           kd-sub (K5[.., a]) (K6[.., a])),
      Dwf : (g, a : con, y591 : cn-of a (K4[..]) |- kd-wf (K6[.., a])),
      Dwf1 : (g |- kd-wf K3),
      Dwf2 : (g |- kd-wf K4)
    | x : [g |- kd-sub (sigma K3 (\z522. K5)) (sigma K4 (\z522. K6))]
    ; split
      kd-sub-reg
        [_, b : block (a : con, da : cn-of a (K3[..])) |- Dsub1[.., b.1, b.2]] as
      case kd-sub-reg/i:
      { g : conbind-reg,
        K3 : (g |- kind),
        K7 : (g, a : con |- kind),
        K4 : (g |- kind),
        K8 : (g, a : con |- kind),
        Dsub : (g |- kd-sub K3 K4),
        Dsub1 :
          (g, a : con, x591 : cn-of a (K3[..]) |-
             kd-sub (K7[.., a]) (K8[.., a])),
        Dwf : (g, a : con, y591 : cn-of a (K4[..]) |- kd-wf (K8[.., a])),
        Dwf1 : (g |- kd-wf K3),
        Dwf2 : (g |- kd-wf K4),
        Dwf3 : (g, a : con, z5 : cn-of a (K3[..]) |- kd-wf (K7[.., a])),
        Dwf4 : (g, a : con, y6 : cn-of a (K3[..]) |- kd-wf (K8[.., a]))
      | x : [g |- kd-sub (sigma K3 (\z522. K7)) (sigma K4 (\z522. K8))]
      ; solve
          [_ |-
             kd-sub-reg/i (kd-wf/sigma Dwf1 (\a. \da. Dwf3))
             (kd-wf/sigma Dwf2 (\a. \da. Dwf))]
      }
    }
  }
  case kd-sub/pi:
  { g : conbind-reg,
    K4 : (g |- kind),
    K5 : (g, a9 : con |- kind),
    K3 : (g |- kind),
    K6 : (g, a10 : con |- kind),
    Dsub : (g |- kd-sub K3 K4),
    Dsub1 :
      (g, a : con, z589 : cn-of a (K3[..]) |- kd-sub (K5[.., a]) (K6[.., a])),
    Dwf : (g, a : con, x589 : cn-of a (K4[..]) |- kd-wf (K5[.., a]))
  | x : [g |- kd-sub (pi K4 (\z521. K5)) (pi K3 (\z521. K6))]
  ; split kd-sub-reg [_ |- Dsub] as
    case kd-sub-reg/i:
    { g : conbind-reg,
      K4 : (g |- kind),
      K5 : (g, a9 : con |- kind),
      K3 : (g |- kind),
      K6 : (g, a10 : con |- kind),
      Dsub : (g |- kd-sub K3 K4),
      Dsub1 :
        (g, a : con, z589 : cn-of a (K3[..]) |-
           kd-sub (K5[.., a]) (K6[.., a])),
      Dwf : (g, a : con, x589 : cn-of a (K4[..]) |- kd-wf (K5[.., a])),
      Dwf1 : (g |- kd-wf K3),
      Dwf2 : (g |- kd-wf K4)
    | x : [g |- kd-sub (pi K4 (\z521. K5)) (pi K3 (\z521. K6))]
    ; split
      kd-sub-reg
        [_, b : block (a : con, da : cn-of a (K3[..])) |- Dsub1[.., b.1, b.2]] as
      case kd-sub-reg/i:
      { g : conbind-reg,
        K4 : (g |- kind),
        K7 : (g, a : con |- kind),
        K3 : (g |- kind),
        K8 : (g, a : con |- kind),
        Dsub : (g |- kd-sub K3 K4),
        Dsub1 :
          (g, a : con, z589 : cn-of a (K3[..]) |-
             kd-sub (K7[.., a]) (K8[.., a])),
        Dwf : (g, a : con, x589 : cn-of a (K4[..]) |- kd-wf (K7[.., a])),
        Dwf1 : (g |- kd-wf K3),
        Dwf2 : (g |- kd-wf K4),
        Dwf3 : (g, a : con, x12 : cn-of a (K3[..]) |- kd-wf (K7[.., a])),
        Dwf4 : (g, a : con, z12 : cn-of a (K3[..]) |- kd-wf (K8[.., a]))
      | x : [g |- kd-sub (pi K4 (\z521. K7)) (pi K3 (\z521. K8))]
      ; solve
          [_ |-
             kd-sub-reg/i (kd-wf/pi Dwf2 (\a. \da. Dwf))
             (kd-wf/pi Dwf1 (\a. \da. Dwf4))]
      }
    }
  }
  case kd-sub/sing-t:
  { g : conbind-reg, C : (g |- con), Dof : (g |- cn-of C t)
  | x : [g |- kd-sub (sing C) t]
  ; solve [_ |- kd-sub-reg/i (kd-wf/sing Dof) kd-wf/t]
  }
  case kd-sub/trans:
  { g : conbind-reg,
    K1 : (g |- kind),
    K2 : (g |- kind),
    K4 : (g |- kind),
    Dsub : (g |- kd-sub K1 K4),
    Dsub1 : (g |- kd-sub K4 K2)
  | x : [g |- kd-sub K1 K2]
  ; split kd-sub-reg [_ |- Dsub] as
    case kd-sub-reg/i:
    { g : conbind-reg,
      K1 : (g |- kind),
      K2 : (g |- kind),
      K4 : (g |- kind),
      Dsub : (g |- kd-sub K1 K4),
      Dsub1 : (g |- kd-sub K4 K2),
      Dwf : (g |- kd-wf K1),
      Dwf1 : (g |- kd-wf K4)
    | x : [g |- kd-sub K1 K2]
    ; split kd-sub-reg [_ |- Dsub1] as
      case kd-sub-reg/i:
      { g : conbind-reg,
        K1 : (g |- kind),
        K2 : (g |- kind),
        K4 : (g |- kind),
        Dsub : (g |- kd-sub K1 K4),
        Dsub1 : (g |- kd-sub K4 K2),
        Dwf : (g |- kd-wf K1),
        Dwf1 : (g |- kd-wf K4),
        Dwf2 : (g |- kd-wf K4),
        Dwf3 : (g |- kd-wf K2)
      | x : [g |- kd-sub K1 K2]
      ; solve [_ |- kd-sub-reg/i Dwf Dwf3]
      }
    }
  }
  case kd-sub/refl:
  { g : conbind-reg,
    K1 : (g |- kind),
    K2 : (g |- kind),
    Dequiv : (g |- kd-equiv K1 K2)
  | x : [g |- kd-sub K1 K2]
  ; split kd-equiv-reg [_ |- Dequiv] as
    case kd-equiv-reg/i:
    { g : conbind-reg,
      K1 : (g |- kind),
      K2 : (g |- kind),
      Dequiv : (g |- kd-equiv K1 K2),
      Dwf : (g |- kd-wf K1),
      Dwf1 : (g |- kd-wf K2)
    | x : [g |- kd-sub K1 K2]
    ; solve [_ |- kd-sub-reg/i Dwf Dwf1]
    }
  }
}

and proof cn-of-reg :
  (g : conbind-reg) [g |- cn-of C K] ->
    [g |- kd-wf K] =
/ total 1 /
intros
{ g : conbind-reg, C : (g |- con), K : (g |- kind)
| x : [g |- cn-of C K]
; split x as
  case cn-of/subsume:
  { g : conbind-reg,
    C : (g |- con),
    K : (g |- kind),
    K1 : (g |- kind),
    Dof : (g |- cn-of C K1),
    Dsub : (g |- kd-sub K1 K)
  | x : [g |- cn-of C K]
  ; split kd-sub-reg [_ |- Dsub] as
    case kd-sub-reg/i:
    { g : conbind-reg,
      C : (g |- con),
      K : (g |- kind),
      K1 : (g |- kind),
      Dof : (g |- cn-of C K1),
      Dsub : (g |- kd-sub K1 K),
      Dwf : (g |- kd-wf K1),
      Dwf1 : (g |- kd-wf K)
    | x : [g |- cn-of C K]
    ; solve [_ |- Dwf1]
    }
  }
  case cn-of/extsigma:
  { g : conbind-reg,
    C : (g |- con),
    K1 : (g |- kind),
    K2 : (g, a23 : con |- kind),
    Dof : (g |- cn-of (pi1 C) K1),
    Dof1 : (g |- cn-of (pi2 C) (K2[.., pi1 C])),
    Dwf : (g, a : con, x602 : cn-of a (K1[..]) |- kd-wf (K2[.., a]))
  | x : [g |- cn-of C (sigma K1 (\z522. K2))]
  ; by cn-of-reg [_ |- Dof] as Dwf1 unboxed;
    solve [_ |- kd-wf/sigma Dwf1 (\a. \da. Dwf)]
  }
  case cn-of/extpi:
  { g : conbind-reg,
    C : (g |- con),
    K1 : (g |- kind),
    K3 : (g, a22 : con |- kind),
    K2 : (g, a21 : con |- kind),
    Dof : (g |- cn-of C (pi K1 (\z521. K2))),
    Dof1 :
      (g, a : con, y601 : cn-of a (K1[..]) |-
         cn-of (app (C[..]) a) (K3[.., a]))
  | x : [g |- cn-of C (pi K1 (\z521. K3))]
  ; split cn-of-reg [_ |- Dof] as
    case kd-wf/pi:
    { g : conbind-reg,
      C : (g |- con),
      K1 : (g |- kind),
      K3 : (g, a22 : con |- kind),
      K2 : (g, a21 : con |- kind),
      Dof : (g |- cn-of C (pi K1 (\z521. K2))),
      Dof1 :
        (g, a : con, y601 : cn-of a (K1[..]) |-
           cn-of (app (C[..]) a) (K3[.., a])),
      Dwf : (g |- kd-wf K1),
      Dwf1 : (g, a : con, y582 : cn-of a (K1[..]) |- kd-wf (K2[.., a]))
    | x : [g |- cn-of C (pi K1 (\z521. K3))]
    ; by cn-of-reg
           [_, b : block (a : con, da : cn-of a (K1[..])) |-
              Dof1[.., b.1, b.2]]
      as Dwf2 unboxed;
      solve [_ |- kd-wf/pi Dwf (\a. \da. Dwf2[.., <a; da>])]
    }
  }
  case cn-of/sing:
  { g : conbind-reg, C : (g |- con), Dof : (g |- cn-of C t)
  | x : [g |- cn-of C (sing C)]
  ; solve [_ |- kd-wf/sing Dof]
  }
  case cn-of/labeled:
  { g : conbind-reg,
    L : ( |- label),
    C1 : (g |- con),
    Dof : (g |- cn-of C1 t)
  | x : [g |- cn-of (labeled L[] C1) t]
  ; solve [_ |- kd-wf/t]
  }
  case cn-of/rec:
  { g : conbind-reg,
    K1 : (g |- kind),
    C1 : (g, a20 : con, a19 : con |- con),
    C2 : (g |- con),
    Dwf : (g |- kd-wf K1),
    Dof :
      (g, a : con, y599 : cn-of a (pi (K1[..]) (\l. t)), b : con,
       x599 : cn-of b (K1[..]) |- cn-of (C1[.., a, b]) t),
    Dof1 : (g |- cn-of C2 K1)
  | x : [g |- cn-of (rec' K1 (\z529. \y530. C1) C2) t]
  ; solve [_ |- kd-wf/t]
  }
  case cn-of/tagged:
  { g : conbind-reg
  | x : [g |- cn-of tagged t]
