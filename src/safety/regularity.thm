    | x : [g |- md-fst (md/lete E C2 (\x. M1)) C],
      fst : [g |- md-fst (md/lete E C2 (\x. M1)) C']
    ; by md-fst-fun [_, x : term |- Dfst] [_, x : term |- Dfst1]
      as Deq unboxed;
      solve [_ |- Deq[.., E]]
