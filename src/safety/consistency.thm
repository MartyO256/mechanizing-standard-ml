% Map Preserves Typing

schema map-bind+invert-bind-for-map-of =
  some [B : etp, K : kind, Dmap : tmap K B, Dwf : ewf B]
  block (
    x : eterm, dx : evof x B,
    a : con, da : cn-of a K,
    at : map a x
  ) +
  some [
    K : kind, K' : kind,
    B : etp,
    DmapK : tmap K B,
    DmapB : tunmap B K',
    DwfK : kd-wf K,
    DwfB : ewf B,
    DequivK : kd-equiv K K'
  ]
  block (
    a : con, da : cn-of a K,
    x : eterm, dx : evof x B,
    at : map a x, xt : unmap x a
