--name purity-eq Deq eq.

LF sg-eq : sg -> sg -> type =
| sg-eq/i : sg-eq S S
;
--name sg-eq Deq eq.

LF store-eq : store -> store -> type =
| store-eq/i : store-eq ST ST
;
--name store-eq Deq eq.

LF sttp-eq : sttp -> sttp -> type =
| sttp-eq/i : sttp-eq F F
;
--name sttp-eq Deq eq.

LF term-eq : term -> term -> type =
