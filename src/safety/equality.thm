% Equalities

LF con-eq : con -> con -> type =
| con-eq/i : con-eq C C
;
--name con-eq Deq eq.

LF entp-eq : entp -> entp -> type =
| entp-eq/i : entp-eq ET ET
;
--name entp-eq Deq eq.

LF entry-eq : entry -> entry -> type =
